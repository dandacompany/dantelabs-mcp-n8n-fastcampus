{
    "mcpServers": {
        "Context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ]
        },
        "DuckDuckGo": {
            "command": "uvx",
            "args": ["duckduckgo-mcp-server"]
        },
        "EXA": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.exa.ai/mcp?exaApiKey=[YOUR_EXA_API_KEY]"
            ]
        },
        "Memory": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-memory"
            ],
            "env": {
                "MEMORY_FILE_PATH": "/Users/dante/workspace/dante-code/mcps/memory.json"
            }
        },
        "PPT": {
            "command": "/your/path/to/Office-PowerPoint-MCP-Server/.venv/bin/python",
            "args": [
                "/your/path/to/Office-PowerPoint-MCP-Server/ppt_mcp_server.py"
            ]
        },
        "Word": {
            "command": "uvx",
            "args": [
                "--from",
                "office-word-mcp-server",
                "word_mcp_server"
            ]
        },
        "Gmail": {
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@shinzo-labs/gmail-mcp",
                "--key",
                "[YOUR_SMITHERY_API_KEY]",
                "--profile",
                "[YOUR_SMITHERY_PROFILE]"
            ]
        },
        "GoogleWorkspace": {
            "command": "node",
            "args": [
                "/path/to/google-workspace-server/build/index.js"
            ],
            "env": {
                "GOOGLE_CLIENT_ID": "[YOUR_GOOGLE_CLIENT_ID]",
                "GOOGLE_CLIENT_SECRET": "[YOUR_GOOGLE_CLIENT_SECRET]",
                "GOOGLE_REFRESH_TOKEN": "[YOUR_GOOGLE_REFRESH_TOKEN]"
            }
        },
        "DesktopCommander": {
            "command": "npx",
            "args": [
                "-y",
                "@wonderwhy-er/desktop-commander"
            ]
        },
        "SequentialThinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "Playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "TimeServer": {
            "command": "uvx",
            "args": [
                "mcp-server-time"
            ]
        },
        "NaverSearch": {
            "command": "npx",
            "args": [
                "-y",
                "@isnow890/naver-search-mcp"
            ],
            "env": {
                "NAVER_CLIENT_ID": "[YOUR_NAVER_CLIENT_ID]",
                "NAVER_CLIENT_SECRET": "[YOUR_NAVER_CLIENT_SECRET]"
            }
        },
        "Linkedin": {
            "command": "uv",
            "args": [
                "--directory",
                "/your/path/to/linkedin-mcp-server",
                "run",
                "main.py"
            ]
        },
        "TalkToFigma": {
            "command": "npx",
            "args": [
                "-y",
                "cursor-talk-to-figma-mcp@latest"
            ]
        },
        "YouTube MCP Server": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "@smithery/cli@latest",
                "run",
                "@icraft2170/youtube-data-mcp-server",
                "--key",
                "[YOUR_SMITHERY_API_KEY]",
                "--profile",
                "[YOUR_SMITHERY_PROFILE]"
            ],
            "env": {}
        },
        "ImageGen": {
            "command": "npx",
            "args": [
                "@gongrzhe/image-gen-server"
            ],
            "env": {
                "REPLICATE_API_TOKEN": "[YOUR_REPLICATE_API_TOKEN]",
                "MODEL": "[YOUR_MODEL]"
            },
            "disabled": false,
            "autoApprove": []
        },
        "ElevenLabs": {
            "command": "uvx",
            "args": [
                "elevenlabs-mcp"
            ],
            "env": {
                "ELEVENLABS_API_KEY": "[YOUR_ELEVENLABS_API_KEY]"
            }
        }
    },
    "isUsingBuiltInNodeForMcp": false
}