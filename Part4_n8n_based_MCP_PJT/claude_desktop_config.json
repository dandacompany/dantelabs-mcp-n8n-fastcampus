{
    "mcpServers": {
        "Playwright": {
            "command": "npx",
            "args": [
                "@playwright/mcp@latest"
            ]
        },
        "DesktopCommander": {
            "command": "npx",
            "args": [
                "-y",
                "@wonderwhy-er/desktop-commander"
            ]
        },
        "SequentialThinking": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-sequential-thinking"
            ]
        },
        "Alpha Vantage": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "https://mcp.alphavantage.co/mcp?apikey=4ATT1ULIVWZOWTMG"
            ]
        },
        "Dante Stock": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:5678/mcp/stock",
                "--header",
                "Authorization: Bearer ${MY_N8N_AUTH_TOKEN}"
            ],
            "env": {
                "MY_N8N_AUTH_TOKEN": "xxxxxxxxxxxxxxxxx"
            }
        },
        "DesignStudio": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:5678/mcp/designstudio"
            ]
        },
        "DataCrawler": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:5678/mcp/crawler"
            ]
        },
        "PostgreSQL": {
            "command": "npx",
            "args": [
                "@henkey/postgres-mcp-server",
                "--connection-string", "postgresql://[USERNAME]:[PASSWORD]@[HOST]:[PORT]/[DATABASE]"
            ]
        },
        "BookScanner": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:5678/mcp/parsing_book"
            ]
        },
        "RAGMaker": {
            "command": "npx",
            "args": [
                "-y",
                "mcp-remote",
                "http://localhost:5678/mcp/rag"
            ]
        }
    },
    "isUsingBuiltInNodeForMcp": false
}